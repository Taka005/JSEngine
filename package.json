{
  "name": "jsengine",
  "version": "1.0.0",
  "description": "Webで動作する軽量な物理エンジン",
  "scripts": {
    "lint": "./node_modules/.bin/eslint ./dist/Engine.ts",
    "docs": "npx typedoc --plugin typedoc-plugin-markdown --name JSEngine --entryPointStrategy expand ./src",
    "build": "npx tsc",
    "bundle": "esbuild --bundle --minify --outfile=./public/Engine.js ./src/Engine.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Taka005/JSEngine.git"
  },
  "author": "Taka",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/Taka005/JSEngine/issues"
  },
  "homepage": "https://github.com/Taka005/JSEngine#readme",
  "dependencies": {
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "esbuild": "^0.21.4",
    "typedoc": "^0.25.13",
    "typedoc-plugin-markdown": "^4.0.3"
  }
}
